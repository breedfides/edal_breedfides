<div class="scrollbar" id="scrollbar" style="min-height:30px;">
	<ol class="tree">
		#foreach($object in $allobjects) #if($object.isDirectory())
		<li class="folder">
			<label for="$object.toString()" style="margin-bottom:5px;">
				<a style="color: #333"
					href="$serverURL/$identifierType/$internalId/$object.getID()/$object.getVersionByDate($date).getRevision()">$object.toString()</a>
				<a	href="$downloadURL/$identifierType/$internalId/$object.getID()/$object.getVersionByDate($date).getRevision()/ZIP" target="_blank">
					<i class="fa fa-download" title="download as zip"></i>
					#if($SizeList.containsKey("$internalId/$object.getID()"))
					$DataSizeClass.of($SizeList.get("$internalId/$object.getID()")).format($SizeList.get("$internalId/$object.getID()"))
					#end
				</a>
			</label>
		</li>
		#else
		<li class="myfile">
			<a style="color: #333" id="$object.toString()"
				href="$serverURL/$identifierType/$internalId/$object.getID()/$object.getVersionByDate($date).getRevision()">$object.toString()</a>
			<a	href="$downloadURL/$identifierType/$internalId/$object.getID()/$object.getVersionByDate($date).getRevision()/DOWNLOAD" target="_blank">
				<i class="fa fa-download" title="download"></i>
				$object.getMetaData().getElementValue($EnumSize).toHTML()
			</a>
		</li>
		#end #end
	</ol>
</div>
<script type="text/javascript">
	window.onload = function()	{
	var divObject =	document.getElementById('scrollbar');
	divObject.scrollTop = divObject.scrollHeight;
	$(function () {
	$("#ballsWaveG").hide();
	});
	}
</script>